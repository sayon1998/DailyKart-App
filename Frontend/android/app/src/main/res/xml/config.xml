<?xml version='1.0' encoding='utf-8'?>
<widget version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <access origin="*" />
  
  <feature name="Permissions">
    <param name="android-package" value="com.android.plugins.Permissions"/>
  </feature>

  <feature name="FirebaseAuthentication">
    <param name="android-package" value="by.chemerisuk.cordova.firebase.FirebaseAuthenticationPlugin"/>
    <param name="onload" value="false"/>
  </feature>

  <feature name="RequestLocationAccuracy">
    <param name="android-package" value="cordova.plugin.RequestLocationAccuracy"/>
  </feature>

  <feature name="SMSReceive">
    <param name="android-package" value="com.andreszs.cordova.sms.SMSReceive"/>
  </feature>

  <feature name="AndroidSmsRetriever">
    <param name="android-package" value="com.codingsans.ionic.smsRetriever.AndroidSmsRetriever"/>
  </feature>

  <feature name="Sms">
    <param name="android-package" value="com.cordova.plugins.sms.Sms"/>
  </feature>

  
</widget>